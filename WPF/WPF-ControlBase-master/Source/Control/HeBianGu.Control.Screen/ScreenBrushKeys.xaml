<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.Control.Screen">

    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.ScreenBrush.Yellow.1, TypeInTargetAssembly={x:Type local:ScreenBrushKeys}}" Color="#fef000" />
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.ScreenBrush.Yellow.2, TypeInTargetAssembly={x:Type local:ScreenBrushKeys}}" Color="#f7e80d" />
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.ScreenBrush.Orange.1, TypeInTargetAssembly={x:Type local:ScreenBrushKeys}}" Color="#f78c44" />

    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.ScreenBrush.Red.1, TypeInTargetAssembly={x:Type local:ScreenBrushKeys}}" Color="#ed405d" />

</ResourceDictionary>

