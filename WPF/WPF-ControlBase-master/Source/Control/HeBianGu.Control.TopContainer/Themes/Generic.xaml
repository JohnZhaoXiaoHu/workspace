<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.Control.TopContainer">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/HeBianGu.Control.TopContainer;component/GitTopContainer.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource {x:Static local:GitTopItem.DynamicKey}}" TargetType="{x:Type local:GitTopItem}" />
    <Style BasedOn="{StaticResource {x:Static local:GitTopContainer.DynamicKey}}" TargetType="{x:Type local:GitTopContainer}" />

</ResourceDictionary>
