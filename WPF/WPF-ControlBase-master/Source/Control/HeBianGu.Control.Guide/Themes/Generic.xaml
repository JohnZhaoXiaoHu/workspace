<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.Control.Guide">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/HeBianGu.Control.Guide;component/GuideHost.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate DataType="{x:Type local:GuideViewPresenter}">
        <Button h:Cattach.Icon="{Binding Icon}" Style="{DynamicResource {x:Static h:MvpKeys.Button}}" />
    </DataTemplate>
</ResourceDictionary>
