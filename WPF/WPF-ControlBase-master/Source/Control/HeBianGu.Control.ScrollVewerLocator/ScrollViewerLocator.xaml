                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.Control.ScrollVewerLocator">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/HeBianGu.Control.ScrollVewerLocator;component/ScrollLocatorBase.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{ComponentResourceKey ResourceId=S.ScrollViewerLocator.Default, TypeInTargetAssembly={x:Type local:ScrollViewerLocator}}" BasedOn="{StaticResource {x:Static local:ScrollLocatorBase.DefaultKey}}" TargetType="{x:Type local:ScrollViewerLocator}" />
    <Style BasedOn="{StaticResource {x:Static local:ScrollViewerLocator.DefaultKey}}" TargetType="{x:Type local:ScrollViewerLocator}" />
</ResourceDictionary>
