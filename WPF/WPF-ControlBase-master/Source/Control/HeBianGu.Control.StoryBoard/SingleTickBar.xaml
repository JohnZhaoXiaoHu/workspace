<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.Control.StoryBoard">

    <Style x:Key="S.SingleTickBar.Default" TargetType="{x:Type local:SingleTickBar}">
        <Setter Property="Foreground" Value="{DynamicResource {x:Static h:BrushKeys.ForegroundDefault}}" />
        <Setter Property="SplitValue" Value="10" />
        <Setter Property="Height" Value="{DynamicResource {x:Static h:SystemKeys.RowHeight}}" />
    </Style>

    <Style BasedOn="{StaticResource S.SingleTickBar.Default}" TargetType="{x:Type local:SingleTickBar}" />

</ResourceDictionary>
