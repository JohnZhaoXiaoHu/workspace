<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.Control.Explorer">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/HeBianGu.Control.Explorer;component/Explorer.xaml" />
        <ResourceDictionary Source="pack://application:,,,/HeBianGu.Control.Explorer;component/ExplorerTree.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource {x:Static h:Explorer.SingleKey}}" TargetType="{x:Type local:Explorer}" />
    <Style BasedOn="{StaticResource {x:Static h:ExplorerTree.QuickKey}}" TargetType="{x:Type local:ExplorerTree}" />
</ResourceDictionary>
