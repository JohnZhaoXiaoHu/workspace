<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.General.WpfControlLib">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/HeBianGu.General.WpfControlLib;component/Custom/System/ListBox.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="{ComponentResourceKey ResourceId=S.ScrollToEndListBox.Default, TypeInTargetAssembly={x:Type local:ScrollToEndListBox}}" BasedOn="{StaticResource {x:Static local:ListBoxKeys.Dynamic}}" TargetType="{x:Type local:ScrollToEndListBox}" />

    <Style BasedOn="{StaticResource {x:Static h:ScrollToEndListBox.DefaultKey}}" TargetType="{x:Type local:ScrollToEndListBox}" />
</ResourceDictionary>
