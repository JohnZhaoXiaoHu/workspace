<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:h="https://github.com/HeBianGu"
                    xmlns:local="clr-namespace:HeBianGu.General.WpfControlLib">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="WindowBase.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="S.Window.Main.Default" BasedOn="{StaticResource {x:Static h:WindowBase.DynamicKey}}" TargetType="{x:Type local:MainWindowBase}">
        <Setter Property="MaxboxEnable" Value="True" />
        <Setter Property="MinboxEnable" Value="True" />
        <Setter Property="NotifyIconSource" Value="/HeBianGu.General.WpfControlLib;component/Resources/logo.ico" />
        <Setter Property="Width" Value="1460" />
        <Setter Property="Height" Value="800" />
        <Setter Property="DefaultBlurEffect">
            <Setter.Value>
                <BlurEffect />
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>