<h:LinkWindowBase x:Class="HeBianGu.Template.Link.MainWindow"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                  xmlns:h="https://github.com/HeBianGu"
                  xmlns:local="clr-namespace:HeBianGu.Template.Link"
                  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                  Title="MainWindow"
                  CaptionBackground="{DynamicResource {x:Static h:BrushKeys.Dark0_5}}"
                  CaptionForeground="{DynamicResource {x:Static h:BrushKeys.ForegroundDefault}}"
                  CaptionHeight="50"
                  Style="{DynamicResource {x:Static h:LinkWindowBase.DownLoadKey}}"
                  mc:Ignorable="d">
    <h:LinkWindowBase.TabLinks>
        <h:TabLink Action="Home"
                   Controller="Home"
                   DisplayName="Home"
                   Logo="&#xe6a3;" />

        <h:TabLink Action="Layout"
                   Controller="Layout"
                   DisplayName="Layout"
                   Logo="{x:Null}" />
        
        <h:TabLink Action="Custom"
                   Controller="Custom"
                   DisplayName="Custom"
                   Logo="{x:Null}" />
        
        <h:TabLink Action="Set"
                   Controller="Set"
                   DisplayName="Set"
                   Logo="{x:Null}" />
    </h:LinkWindowBase.TabLinks>
</h:LinkWindowBase>
