<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:HeBianGu.Base.WpfBase"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/HeBianGu.Base.WpfBase;component/Themes/Resource.xaml" />
        <ResourceDictionary Source="/HeBianGu.Base.WpfBase;component/Themes/DrawingGroup.xaml" />
        <ResourceDictionary Source="/HeBianGu.Base.WpfBase;component/Themes/StoryBoard.xaml" />
        <ResourceDictionary Source="/HeBianGu.Base.WpfBase;component/Themes/Color/Light.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!--  Background  -->
    <!--<SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextBackground.Default, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#213466" />-->
    <LinearGradientBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextBackground.Default, TypeInTargetAssembly={x:Type local:BrushKeys}}" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#eceef0" />
            <GradientStop Offset="1" Color="#eceef0" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.RowIndex.BackGround, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#e6ebf9" />

    <!--  Foreground  -->
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextForeground.Title, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#213466" />
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextForeground.Default, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#2e4683" />
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextForeground.MouseOver, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#d7dff3" />
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextForeground.Selected, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#bac7e7" />
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextForeground.Assist, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#3a5495" />
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=S.Brush.TextForeground.Link, TypeInTargetAssembly={x:Type local:BrushKeys}}" Color="#6296d8" />

    <!--  BorderBrush  -->
