                                FontSize="15" />
                        <Button Width="Auto"
                                Height="100"
                                h:Cattach.Icon="&#xe656;"
                                h:Cattach.IconSize="40"
                                h:Cattach.Orientation="Vertical"
                                Background="Transparent"
                                Content="微信"
                                FontSize="15" />
                    </ItemsControl>
                </GroupBox>

            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
