                                Height="100"
                                h:Cattach.Icon="&#xe69e;"
                                h:Cattach.IconSize="40"
                                h:Cattach.Orientation="Vertical"
                                Background="Transparent"
                                Content="微信"
                                FontSize="15" />
                    </ItemsControl>
                </GroupBox>

                <GroupBox h:Cattach.CaptionBackground="Transparent"
                          h:Cattach.GuideParentTitle="探索"
                          h:Cattach.UseGuide="True"
                          Header="酷狗小程序"
                          Style="{DynamicResource {x:Static h:GroupBoxKeys.UnderLine}}">
                    <ItemsControl>
                        <ItemsControl.ItemsPanel>
                            <ItemsPanelTemplate>
                                <UniformGrid Rows="2" />
                            </ItemsPanelTemplate>
                        </ItemsControl.ItemsPanel>
                        <Button Width="Auto"
