        <TabItem Header="Screen3">
            <h:Screen>
                <h:Interaction.Triggers>
                    <h:EventTrigger EventName="Loaded">
                        <h:InvokeTimeSplitAnimationAction IsUseAll="False" SplitMilliSecond="-700">
                            <h:InvokeTimeSplitAnimationAction.Timelines>
                                <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)"
                                                 From="1200" To="0"
                                                 Duration="00:00:01" />
                                <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"
                                                 From="0" To="1"
                                                 Duration="00:00:01" />
                                <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
                                                 From="0" To="1"
                                                 Duration="00:00:01" />
                                <DoubleAnimation Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[2].(RotateTransform.Angle)"
                                                 From="720" To="0"
                                                 Duration="00:00:01" />
                            </h:InvokeTimeSplitAnimationAction.Timelines>
                        </h:InvokeTimeSplitAnimationAction>
                    </h:EventTrigger>
                </h:Interaction.Triggers>
                <h:Screen.Resources>
                    <ResourceDictionary Source="pack://application:,,,/HeBianGu.Control.Screen;component/Themes/ScreenStyle2.xaml" />
                </h:Screen.Resources>
