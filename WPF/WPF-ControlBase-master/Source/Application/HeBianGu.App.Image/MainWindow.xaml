<h:MainWindow x:Class="HeBianGu.App.Image.MainWindow"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
              xmlns:h="https://github.com/HeBianGu"
              xmlns:local="clr-namespace:HeBianGu.App.Image"
              xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
              Title="H图库"
              Width="1200"
              Height="760"
              h:Cattach.Icon=""
              CaptionBackground="{DynamicResource {x:Static h:BrushKeys.Dark0_4}}"
              CaptionForeground="{DynamicResource {x:Static h:BrushKeys.ForegroundTitle}}"
              CaptionHeight="50"
              DataContext="{Binding Source={StaticResource S.DataSource.Locator}, Path=ShellViewModel}"
              Icon="/HeBianGu.General.WpfControlLib;component/Resources/logo.ico"
              IconSize="30"
              NotifyIconSource="/HeBianGu.General.WpfControlLib;component/Resources/logo.ico"
              mc:Ignorable="d">
    <h:Cattach.CaptionLeftTemplate>
        <ControlTemplate>
            <DockPanel Margin="10,0">
                <ToggleButton HorizontalAlignment="Left"
                              h:Cattach.CheckedIcon="{x:Static h:IconAll.Left_Arrow}"
                              h:Cattach.UncheckedIcon="{x:Static h:IconAll.Menu1}"
                              IsChecked="{Binding IsMenuExpanded}"
                              Style="{DynamicResource {x:Static h:ToggleButtonKeys.Dynamic}}"
                              ToolTip="显示/隐藏" />
            </DockPanel>
        </ControlTemplate>
    </h:Cattach.CaptionLeftTemplate>
    <h:Cattach.CaptionCenterTemplate>
        <ControlTemplate>
            <DockPanel HorizontalAlignment="Right" Background="Transparent">
                <ItemsControl ItemsSource="{Binding ElementName=box, Path=CommandBindings}" Visibility="{Binding IsViewState, Converter={x:Static h:Converter.GetTrueToVisible}}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Button Margin="2"
                                    VerticalAlignment="Center"
                                    h:Cattach.Icon="{Binding Command.Icon}"
                                    Command="{Binding Command}"
                                    CommandTarget="{Binding ElementName=box}"
                                    Style="{DynamicResource {x:Static h:ButtonKeys.Icon}}"
                                    ToolTip="{Binding Command.Text}" />
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <UniformGrid Rows="1" />
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </DockPanel>
        </ControlTemplate>
    </h:Cattach.CaptionCenterTemplate>
    <h:Cattach.LeftTemplate>
        <ControlTemplate>
            <DockPanel Width="250" Background="{DynamicResource {x:Static h:BrushKeys.Dark0_2}}" Visibility="{Binding Path=IsMenuExpanded, Converter={x:Static h:Converter.GetTrueToVisible}}">
                <GroupBox Margin="2,0"
                          Padding="0"
                          Background="Transparent"
                          DockPanel.Dock="Top"
                          Header="我的图库"
                          Style="{DynamicResource {x:Static h:GroupBoxKeys.Title}}">
                    <h:Cattach.CaptionRightTemplate>
                        <ControlTemplate>
                            <DockPanel HorizontalAlignment="Right">
                                <Button Margin="0"
                                        h:Cattach.Icon="{x:Static h:Icons.Add}"
                                        Style="{DynamicResource {x:Static h:ButtonKeys.AddTransparent}}"
                                        ToolTip="创建资源库" />
                                <Button Margin="0"
                                        h:Cattach.Icon="{x:Static h:Icons.Search}"
                                        Style="{DynamicResource {x:Static h:ButtonKeys.AddTransparent}}"
                                        ToolTip="切换资源库" />
                            </DockPanel>
                        </ControlTemplate>
                    </h:Cattach.CaptionRightTemplate>
                    <ListBox>
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <DockPanel LastChildFill="False">
                                    <TextBlock Margin="10,0" Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="{Binding Value1}" />
                                    <TextBlock Text="{Binding Value}" />
                                    <TextBlock DockPanel.Dock="Right" Text="{Binding Value2}" />
                                </DockPanel>
                            </DataTemplate>
                        </ListBox.ItemTemplate>

                        <h:TestViewModel Value1="{x:Static h:Icons.Alarm}" Value2="343" Value="全部" />
                        <h:TestViewModel Value1="{x:Static h:Icons.Film}" Value2="25" Value="未分类" />
                        <h:TestViewModel Value1="{x:Static h:Icons.Edit}" Value2="1" Value="未标签" />
                        <h:TestViewModel Value1="{x:Static h:Icons.Tag}" Value2="" Value="标签管理" />
                        <h:TestViewModel Value1="{x:Static h:Icons.Add}" Value2="11" Value="回收站" />
                        <h:TestViewModel Value1="{x:Static h:Icons.Redo}" Value2="" Value="最近使用" />
                    </ListBox>
                </GroupBox>
                <GroupBox Margin="2,0"
                          Padding="0"
                          Background="Transparent"
                          Header="文件夹"
                          Style="{DynamicResource {x:Static h:GroupBoxKeys.Title}}">
                    <h:Cattach.CaptionRightTemplate>
                        <ControlTemplate>
                            <Button HorizontalAlignment="Right" Style="{DynamicResource {x:Static h:ButtonKeys.AddTransparent}}" />
                        </ControlTemplate>
                    </h:Cattach.CaptionRightTemplate>

                    <TreeView VerticalAlignment="Stretch"
                              h:Cattach.SearchDock="Bottom"
                              Background="Transparent"
                              BorderThickness="0">
                        <TreeView.ItemTemplate>
                            <HierarchicalDataTemplate ItemsSource="{Binding Nodes}">
                                <DockPanel>
                                    <DockPanel LastChildFill="False">
                                        <TextBlock Margin="0,0,5,0" Style="{DynamicResource {x:Static h:TextBlockKeys.Icon}}" Text="{Binding Value1}" />
                                        <TextBlock Text="{Binding Value}" />
                                    </DockPanel>
                                </DockPanel>
                            </HierarchicalDataTemplate>
                        </TreeView.ItemTemplate>
                        <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="343" Value="海报设计">
                            <h:TestViewModel.Nodes>
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="扁平风格">
                                    <h:TestViewModel.Nodes>
                                        <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="大众" />
                                        <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="小众" />
                                    </h:TestViewModel.Nodes>
                                </h:TestViewModel>

                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="收回风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="渐变风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="立体风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="极简风格" />
                            </h:TestViewModel.Nodes>
                        </h:TestViewModel>
                        <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="343" Value="插画设计">
                            <h:TestViewModel.Nodes>
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="扁平风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="收回风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="渐变风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="立体风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="极简风格" />
                            </h:TestViewModel.Nodes>
                        </h:TestViewModel>
                        <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="343" Value="包装设计">
                            <h:TestViewModel.Nodes>
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="扁平风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="收回风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="渐变风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="立体风格" />
                                <h:TestViewModel Value1="{x:Static h:Icons.Folder}" Value2="11" Value="极简风格" />
                            </h:TestViewModel.Nodes>
                        </h:TestViewModel>
                    </TreeView>
                </GroupBox>
            </DockPanel>
        </ControlTemplate>
    </h:Cattach.LeftTemplate>
