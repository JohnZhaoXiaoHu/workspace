<UserControl x:Class="HeBianGu.App.Cross.View.Custom.HomeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:h="https://github.com/HeBianGu"
             xmlns:local="clr-namespace:HeBianGu.App.Cross.View.Custom"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Border Margin="30" Background="{Binding RelativeSource={RelativeSource AncestorType={x:Type h:CrossManagerWindowBase}}, Path=CurrentLink.Tag}" CornerRadius="500" />
</UserControl>
