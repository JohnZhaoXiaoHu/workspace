    <ScrollViewer>

        <ScrollViewer.Resources>
            <Style BasedOn="{StaticResource {x:Static h:ButtonKeys.Transparent}}" TargetType="Button">
                <Setter Property="RenderTransform" Value="{StaticResource {x:Static h:SystemKeys.TransformGroup}}" />
                <Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
                <Setter Property="Width" Value="Auto" />
                <Setter Property="Height" Value="Auto" />
                <Setter Property=" h:Cattach.IconSize" Value="150" />
                <Setter Property="FontSize" Value="150" />
                <!--<Setter Property="IsHitTestVisible" Value="False"/>-->
            </Style>
        </ScrollViewer.Resources>
        <StackPanel>
            <GroupBox Height="500" Header="随机+放大+From">
                <Grid>
                    <Grid Width="400" Height="400">
                        <h:Interaction.Triggers>
                            <h:EventTrigger EventName="Loaded">
                                <h:InvokeRandomSplitAnimationAction RepeatBehavior="Forever" SplitMilliSecond="-700">
                                    <h:InvokeRandomSplitAnimationAction.Timelines>
                                        
                                            <h:RandomDoubleAnimation Max="3" Min="1"
                                                                     UseFrom="False"
                                                                     UseTo="True"
                                                                     Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)"
                                                                     From="0"
                                                                     Duration="00:00:01">
                                                <DoubleAnimation.EasingFunction>
                                                    <ElasticEase />
                                                </DoubleAnimation.EasingFunction>
                                            </h:RandomDoubleAnimation>

                                            <h:RandomDoubleAnimation Max="3" Min="1"
                                                                     UseFrom="False"
                                                                     UseTo="True"
                                                                     Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)"
