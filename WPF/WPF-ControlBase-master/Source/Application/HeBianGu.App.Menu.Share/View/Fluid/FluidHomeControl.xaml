<UserControl x:Class="HeBianGu.App.Menu.View.Fluid.FluidHomeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:h="https://github.com/HeBianGu"
             xmlns:local="clr-namespace:HeBianGu.App.Menu.View.Fluid"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <Grid Background="Transparent">
        <h:LinkActionsContianer Style="{DynamicResource {x:Static h:LinkActionsContianer.SingleHorizontalLeftBottomKey}}">
            <h:LinkActionCollection>
                <h:LinkAction Action="FluidUniform"
                              Controller="Fluid"
                              DisplayName="Uniform"
                              Logo="&#xe827;" />
                <h:LinkAction Action="FluidStack"
                              Controller="Fluid"
                              DisplayName="StackPanel"
                              Logo="&#xe836;" />
            </h:LinkActionCollection>
        </h:LinkActionsContianer>
    </Grid>
</UserControl>
